TABLE USER
	id : SYSTEM_ADMIN
	pw : absolute0922
	
TABLE NAME
	- slave
		 1) sNo - NUMBER(100) [NN] [PK] [SEQ]
		 2) sName - VARCHAR2(30) [NN] 
		 3) sEmail - VARCHAR2(50) [NN] [UK]
		 4) sPwd - VARCHAR2(100) [NN]
	 - board
	 	 1) bNo - NUMBER(100) [NN] [PK] [SEQ]
	 	 2) sNo (외래키) - NUMBER(100) [FK]
		 3) bTitle - VARCHAR2(50) [NN]
		 4) bContents - VARCHAR2(500) [NN]
		 5) bCreateAt - DATE [default sysdate]
		 6) bViews - NUMBER(100) [default 0]
		 
		 
